{
  "name": "remote-pp",
  "version": "0.0.1",
  "description": "distributed pair programming timer",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Morgan Bentell",
  "license": "MIT",
  "repository": "https://github.com/agentbellnorm",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.17.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.1",
    "elm-webpack-loader": "^6.0.1",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
